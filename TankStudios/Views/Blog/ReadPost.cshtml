@model TankStudios.Models.ReadPostModel

@{
    ViewBag.Title = Model.Title;
}

<header>
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <img class="img-responsive img-circle" src=@Model.ImageLink alt="">
                <div class="intro-text">
                    <span class="name">@Model.Title</span>
                    <hr>
                    <span class="skills">@Model.ImageLink</span><br />
                </div>
            </div>
        </div>
    </div>
</header>
<section>
    <div class="container">
        <div  class="row" style="padding: 5px;">
            @Html.Raw(Model.Content);
        </div>
    </div>
</section>